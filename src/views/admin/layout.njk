    </footer>
  </div>
</body>
<script type="text/javascript" nonce="{{ nonce }}">
const createRoute = (routeName, routePath = '') => {
  let route;
  const appRoute = JSON.parse('{{ appRoute | json }}');

  switch (routeName) {
    case 'web':
      route = appRoute.web + routePath;
      break;

    case 'api':
      route = appRoute.api + routePath;
      break;

    case 'admin':
      route = appRoute.admin + routePath;
      break;
  }

  return route.replace(/\/{2,}/g, '/');
};
</script>
